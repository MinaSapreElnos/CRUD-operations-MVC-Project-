@model RoleViewMOdel 
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h1>Details @Model.RoleName </h1>

<div class="mb-3">
    <label class="form-label" asp-for="Id"></label>
    <input type="text" readonly class="form-control" asp-for="Id">
    <span asp-validation-for="Id" class="alert-danger mt-1"></span>
</div>
<div class="mb-3">
    <label class="form-label" asp-for="RoleName"></label>
    <input type="text" readonly class="form-control" id="exampleInputPassword1" asp-for="RoleName">
    <span asp-validation-for="RoleName" class=" alert-danger mt-1"></span>
</div>



<a asp-action="Edit" asp-route-id="@Model.Id"class="btn btn-secondary">Edit</a>
<a asp-action="Index"  class="btn btn-light">Back to Index</a>


